{
  "ConnectionStrings": {
    "DefaultConnection": "server=(LocalDb)\\MSSQLLocalDB;database=League_Master;Trusted_Connection=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },

    "ClarivateFile": {
      "Options": {
        "FolderPath": "..//Logs",
        "FilePath": "ClarivateLog_{date}.log"
      },
      "LogLevel": {
        "RoundTheCode.HostedServices.Web.Controllers": "Information",
        "RoundTheCode.HostedServices.Web.HostedServices": "Information"
      }
    },
    "AllowedHosts": "*"
  }
}
